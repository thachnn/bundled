{
  "name": "npm-package-bundler",
  "version": "4.46.0-webpack.0",
  "description": "Bundle NPM package webpack v4 by using itself",
  "keywords": [
    "npm",
    "yarn",
    "package",
    "module",
    "bundle",
    "bundled",
    "rollup",
    "webpack"
  ],
  "repository": "thachnn/bundled",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "clean": "git clean -fdx dist/ node_modules/.cache",
    "postinstall": "git apply --reject --unidiff-zero --whitespace=nowarn scripts/fix-deps.patch",
    "terser": "node node_modules/terser/bin/terser --config-file scripts/terser.config.json",
    "rollup": "node node_modules/rollup/dist/bin/rollup -c --configName",
    "test": "grep -r --include=*.js -E \"[\\[,] ,|__webpack_require__\\.[a-z]|_WEBPACK_|webpackPoly|\\b_(iterator|interop)\" dist"
  },
  "dependencies": {
    "copy-webpack-plugin": "^5.0.5",
    "i18n-webpack-plugin": "^1.0.0",
    "string-replace-loader": "^2.2.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "rollup": "^2.0.6"
  },
  "resolutions": {
    "**/assert/util": "^0.11.0",
    "**/define-property": "^2.0.2",
    "**/extend-shallow": "^3.0.2",
    "**/pumpify/pump": "^3.0.0",
    "**/snapdragon/source-map": "^0.6.0",
    "**/unset-value/has-value": "^1.0.0",
    "**/watchpack-chokidar2/chokidar": "^3.4.1",
    "string-replace-loader/schema-utils": "^1.0.0",
    "webpack/**/terser": "terser/terser#v4.8.1",
    "webpack/memory-fs": "^0.5.0"
  }
}
